FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       dictionary;
    location    "system";
    object      fvOptions;
}

limitTemperature
{
    type        limitTemperature;
    active      true;
    limitTemperatureCoeffs
    {
        selectionMode all;
        min    10;
        max    100000;
    }
}

