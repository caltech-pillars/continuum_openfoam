FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 100;

boundaryField
{
    NozzleOutlet
    {
        type            fixedValue;
        value           uniform 900;
    }
    Ground
    {
        type            fixedValue;
        value           uniform 100;
    }
    InviscidWall1
    {
        type            wedge;
    }
    InviscidWall2
    {
        type            wedge;
    }
    BagWall
    {
        type            zeroGradient;
    }
    FreeStream
    {
        type            waveTransmissive;
        gamma           1.4;
    }
    RocketWall
    {
        type            zeroGradient;
    }
}


